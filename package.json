{
  "name": "stream-alerts",
  "version": "0.1.9",
  "main": "dist/main/index.js",
  "description": "An electron-based app for managing twitch stream sound alerts",
  "author": "zThundy__",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zThundy/SoundApp.git"
  },
