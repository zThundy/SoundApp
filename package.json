    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "bufferutil": "^4.0.9",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "electron-store": "^11.0.2",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
    "typescript": "^5.9.3",
